# Production environment for minimal memory usage
USE_MINIMAL_SERVER=true
NODE_ENV=production
LOG_LEVEL=error

# Disable all non-essential features
ENABLE_REDIS=false
ENABLE_QUEUE=false
ENABLE_HEALTH_CHECKS=false
ENABLE_METRICS=false
ENABLE_PERFORMANCE_MONITORING=false
ENABLE_AUDIT_LOGGING=false
ENABLE_ANALYTICS=false

# Minimal settings
MAX_SESSIONS=50
SESSION_TTL=3600000
MAX_WS_CONNECTIONS=20