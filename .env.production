# Production environment for optimized memory usage
NODE_ENV=production
PORT=10000

# Feature flags for progressive enhancement
ENABLE_AGENTS=true
ENABLE_WEBSOCKET=false
ENABLE_REDIS=false
ENABLE_MONITORING=false
MEMORY_LIMIT=512

# Logging
LOG_LEVEL=error

# Session settings
MAX_SESSIONS=100
SESSION_TTL=3600000

# Performance settings
MAX_CONCURRENT_REQUESTS=20
REQUEST_TIMEOUT=30000