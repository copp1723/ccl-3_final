{
  "name": "ccl3-swarm",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development tsx server/index-optimized.ts",
    "dev:all": "npx concurrently \"npm run dev\" \"cd client && npm run dev\"",
    "dev:backend": "NODE_ENV=development tsx server/index.ts",
    "dev:frontend": "cd client && npm run dev",
    "build": "./scripts/render-build-optimized.sh",
    "build:dev": "npm run build:client && npm run build:server",
    "build:client": "cd client && npm install && npm run build",
    "build:server": "esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "NODE_ENV=production node dist/index-optimized.js",
    "start:optimized": "NODE_ENV=production node dist/index-optimized.js",
    "dev:optimized": "NODE_ENV=development tsx server/index-optimized.ts",
    "build:optimized": "./scripts/render-build-optimized.sh",
    "remove:heavy": "npm uninstall mailgun.js twilio bullmq express-prometheus-middleware prom-client connect-redis rate-limit-redis memorystore",
    "clean:deps": "npm run remove:heavy && npm install",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx server/db/migrate.ts",
    "db:fix": "node scripts/database/production-migration.js",
    "db:check": "node scripts/database/check-db-status.js",
    "test": "vitest",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "test:system": "node scripts/test-system.js",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix"
  },
